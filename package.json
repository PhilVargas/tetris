{
  "name": "tetris",
  "version": "1.0.0",
  "description": "Coffeescript tetris game using React + Flux",
  "main": "index.js",
  "scripts": {
    "test": "NODE_PATH=./src:./node_modules ./node_modules/jest-cli/bin/jest.js --verbose"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/PhilVargas/tetris.git"
  },
  "author": "Phil Vargas",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/PhilVargas/tetris/issues"
  },
  "homepage": "https://github.com/PhilVargas/tetris",
  "jest": {
    "rootDir": "./src",
    "scriptPreprocessor": "<rootDir>/__tests__/preprocessor.js",
    "moduleFileExtensions": [
      "js",
      "coffee"
    ],
    "modulePathIgnorePatterns": [
      "/node_modules/"
    ],
    "unmockedModulePathPatterns": [
      "piece-map",
      "react",
      "jquery",
      "classnames",
      "object-assign"
    ],
    "testFileExtensions": [
      "coffee",
      "js"
    ],
    "testPathIgnorePatterns": [
      "preprocessor.js"
    ],
    "testPathDirs": [
      "<rootDir>/stores/__tests__"
    ]
  },
  "devDependencies": {
    "browserify": "^10.2.4",
    "coffee-reactify": "^4.0.0",
    "coffee-script": "^1.9.3",
    "gulp": "^3.9.0",
    "gulp-uglify": "^1.2.0",
    "jest": "^0.1.40",
    "jest-cli": "^0.4.17",
    "path": "^0.11.14",
    "vinyl-buffer": "^1.0.0",
    "vinyl-source-stream": "^1.1.0",
    "watchify": "^3.2.3"
  },
  "dependencies": {
    "classnames": "^2.1.3",
    "flux": "^2.0.3",
    "gulp-sass": "^2.0.4",
    "jquery": "^2.1.4",
    "microevent-github": "^1.0.0",
    "object-assign": "^3.0.0",
    "react": "^0.13.3",
    "reactify": "^1.1.1"
  }
}
