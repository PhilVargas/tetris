{
  "name": "tetris",
  "version": "3.0.0",
  "private": true,
  "decription": "TypeScript Tetris game built with RxJS",
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.26",
    "@fortawesome/free-brands-svg-icons": "^5.12.0",
    "@fortawesome/free-solid-svg-icons": "^5.12.0",
    "@fortawesome/react-fontawesome": "^0.1.8",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "@types/classnames": "^2.2.9",
    "@types/jest": "^24.0.0",
    "@types/node": "^12.0.0",
    "@types/react": "^16.9.0",
    "@types/react-dom": "^16.9.0",
    "classnames": "^2.2.6",
    "node-sass": "^4.13.0",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-scripts": "3.3.0",
    "rxjs": "^6.5.4",
    "typescript": "~3.7.2"
  },
  "author": "Phil Vargas",
  "license": "MIT",
  "homepage": "http://philvargas.github.io/tetris",
  "scripts": {
    "predeploy": "yarn run build",
    "deploy": "gh-pages -d build",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/PhilVargas/tetris.git"
  },
  "author": "Phil Vargas",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/PhilVargas/tetris/issues"
  },
  "homepage": "https://github.com/PhilVargas/tetris",
  "jest": {
    "rootDir": "./src",
    "scriptPreprocessor": "<rootDir>/__tests__/preprocessor.js",
    "moduleFileExtensions": [
      "js",
      "coffee"
    ],
    "modulePathIgnorePatterns": [
      "/node_modules/"
    ],
    "unmockedModulePathPatterns": [
      "helpers",
      "react",
      "jquery",
      "classnames",
      "object-assign"
    ],
    "testFileExtensions": [
      "coffee",
      "js"
    ],
    "testPathIgnorePatterns": [
      "preprocessor.js"
    ],
    "testPathDirs": [
      "<rootDir>/stores/__tests__",
      "<rootDir>/actions/__tests__",
      "<rootDir>/helpers/__tests__"
    ]
  },
  "devDependencies": {
    "gh-pages": "^2.2.0"
  }
}
